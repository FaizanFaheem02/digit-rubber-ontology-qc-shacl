@prefix : <https://www.tib.eu/digitrubber/shapes#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .

:ClassMoreThanOneCurationStatusShape
    a sh:NodeShape ;
    sh:targetSubjectsOf obo:IAO_0000114 ;
    sh:severity sh:Violation ;

    sh:property [
        sh:path obo:IAO_0000114 ;
        sh:maxCount 1 ;
        sh:message "Class has more than one curation status (obo:IAO_0000114)." ;
    ] .
