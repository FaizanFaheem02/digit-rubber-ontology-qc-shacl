@prefix :   <https://www.tib.eu/digitrubber/shapes#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix pav: <http://purl.org/pav/> .

:ClassMoreThanOneLastUpdateOnShape
    a sh:NodeShape ;
    sh:targetSubjectsOf pav:lastUpdateOn ;
    sh:severity sh:Violation ;

    sh:property [
        sh:path pav:lastUpdateOn ;
        sh:maxCount 1 ;
        sh:message "Class has more than one pav:lastUpdateOn value." ;
    ] .
