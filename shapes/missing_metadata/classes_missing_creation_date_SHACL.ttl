@prefix :         <https://www.tib.eu/digitrubber/shapes#> .
@prefix sh:       <http://www.w3.org/ns/shacl#> .
@prefix rdf:      <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:      <http://www.w3.org/2002/07/owl#> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .

:DigitRubberClassMissingCreationDateShape
    a sh:NodeShape ;
    sh:severity sh:Violation ;

    sh:target [
        a sh:SPARQLTarget ;
        sh:select """
            SELECT ?this WHERE {
              ?this rdf:type owl:Class .
              FILTER (STRSTARTS(STR(?this), "http://purl.obolibrary.org/obo/DIGITRUBBER_"))
            }
        """ ;
    ] ;

    sh:property [
        sh:path oboInOwl:creation_date ;
        sh:minCount 1 ;
        sh:message "DIGITRUBBER class is missing oboInOwl:creation_date." ;
    ] .

  